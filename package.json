{
  "name": "miliano-e-wallet",
  "version": "1.0.0",
  "description": "Miliano eWallet for RSSB Assessment",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/index ",
    "build": "babel src --out-dir dist",
    "dev": "nodemon --exec babel-node src",
    "test": "cross-env NODE_ENV=test yarn migrate && jest --coverage",
    "test:dev": "jest --watch",
    "migrate": "sequelize db:migrate",
    "undo:last": "sequelize db:migrate:undo",
    "undo:migrations": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/register": "^7.18.9",
    "@babel/runtime": "^7.20.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "sequelize": "^6.37.1",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.4",
    "eslint": "^7.5",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpirumvaa/miliano-e-wallet.git"
  },
  "keywords": [
    "eWallet"
  ],
  "author": "Jean Pierre",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jpirumvaa/miliano-e-wallet/issues"
  },
  "homepage": "https://github.com/jpirumvaa/miliano-e-wallet#readme"
}
